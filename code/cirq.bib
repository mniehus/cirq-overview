	urldate = {2018-08-05},
	date = {2014-06-18},
	eprinttype = {arxiv},
	eprint = {1406.4920},
	keywords = {Quantum Physics},
	file = {arXiv\:1406.4920 PDF:/home/ryan/Zotero/storage/SZAIRQ2I/Poulin et al. - 2014 - The Trotter Step Size Required for Accurate Quantu.pdf:application/pdf;arXiv.org Snapshot:/home/ryan/Zotero/storage/UME3GP68/1406.html:text/html}
}

