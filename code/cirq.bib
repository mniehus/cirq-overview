	file = {arXiv\:1806.08781 PDF:/home/ryan/Zotero/storage/H3SHURZZ/Bausch and Leditzky - 2018 - Quantum Codes from Neural Networks.pdf:application/pdf;arXiv.org Snapshot:/home/ryan/Zotero/storage/C3RKDIVN/1806.html:text/html}
}

