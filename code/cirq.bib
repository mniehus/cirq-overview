@article{lloyd_turing_2012,
	title = {A Turing test for free will},
	volume = {370},
	issn = {1364-503X, 1471-2962},
	url = {http://arxiv.org/abs/1310.3225},
	doi = {10.1098/rsta.2011.0331},
	abstract = {Before Alan Turing made his crucial contributions to the theory of computation, he studied the question of whether quantum mechanics could throw light on the nature of free will. This article investigates the roles of quantum mechanics and computation in free will. Although quantum mechanics implies that events are intrinsically unpredictable, the ‘pure stochasticity’ of quantum mechanics adds only randomness to decision making processes, not freedom. By contrast, the theory of computation implies that even when our decisions arise from a completely deterministic decision-making process, the outcomes of that process can be intrinsically unpredictable, even to – especially to – ourselves. I argue that this intrinsic computational unpredictability of the decision making process is what give rise to our impression that we possess free will. Finally, I propose a ‘Turing test’ for free will: a decision maker who passes this test will tend to believe that he, she, or it possesses free will, whether the world is deterministic or not.},
	pages = {3597--3610},
	number = {1971},
	journaltitle = {Philosophical Transactions of the Royal Society A: Mathematical, Physical and Engineering Sciences},
	author = {Lloyd, Seth},
	urldate = {2018-05-28},
	date = {2012-07-28},
	langid = {english},
	eprinttype = {arxiv},
	eprint = {1310.3225},
	keywords = {Quantum Physics, Computer Science - Artificial Intelligence, Physics - History and Philosophy of Physics},
	file = {Lloyd - 2012 - A Turing test for free will.pdf:/home/ryan/Zotero/storage/XIZ9MXAH/Lloyd - 2012 - A Turing test for free will.pdf:application/pdf}
}

@article{kern_depth_1993,
	title = {On the depth of combinatorial optimization problems},
	volume = {43},
	issn = {0166-218X},
	url = {http://www.sciencedirect.com/science/article/pii/0166218X93900037},
	doi = {10.1016/0166-218X(93)90003-7},
	abstract = {This paper studies the notion of “depth” of a combinatorial optimization problem, which has already been touched on by several other authors (cf. Weber or Hajek). This notion can be defined as follows: Consider a discrete optimization problem min \{c(x{\textbar}x ∈ X\}. Usually hill climbing algorithms work as follows: First, a neighbourhood structure has to be specified on X. Then, starting from an arbitrary solution, say x0, one constructs a sequence x0,x1,x2… (hopefully leading to an optimum) such that xi is a neighbour of xi−1 for all i. For x0 ∈ X, the depth d(x0) is defined to be the smallest d ≥ 0 such that there exists a sequence x0,x1,…,xn with c(xn) {\textless} c(x0) and all intermediate solutions xi having objective value c(xi) ≤ c(x0) + d. The depth of the problem is defined to be the maximum depth of a solution x0 ∈ X. This notion plays an important role in the theory of Simulated Annealing. Here we want to show that the depth is of some interest in its own right. We will prove some upper bounds and investigate the computational complexity of the depth function for some selected examples.},
	pages = {115--129},
	number = {2},
	journaltitle = {Discrete Applied Mathematics},
	shortjournal = {Discrete Applied Mathematics},
	author = {Kern, W.},
	urldate = {2018-05-28},
	date = {1993-05-19},
	keywords = {computational complexity, depth, Hill climbing, Simulated Annealing},
	file = {ScienceDirect Full Text PDF:/home/ryan/Zotero/storage/VJT58QQK/Kern - 1993 - On the depth of combinatorial optimization problem.pdf:application/pdf;ScienceDirect Snapshot:/home/ryan/Zotero/storage/IYP7PG96/0166218X93900037.html:text/html}
}

@article{tayarani-n_landscape_2014,
